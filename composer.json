{
    "name": "fesor/json_matcher",
    "description": "Collection of JSON matchers for painless usage",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Protko",
            "email": "fesors@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
      "psr-4": {"Fesor\\JsonMatcher\\": "src/", "spec\\Fesor\\JsonMatcher\\": "spec/"}
    },
    "require": {
      "php": ">=7.2.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^6.1",
        "friends-of-phpspec/phpspec-code-coverage": "^4.3",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "config": {
      "bin-dir": "bin"
    }
}
